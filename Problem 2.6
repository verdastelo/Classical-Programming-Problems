#include<iostream>
#include<cmath>
#include<string>
#include<vector>
#include<algorithm>
using namespace std;

int main()

{
    double x, y, z = 0;
    cout << "Enter three angles: ";
    cin >> x >> y >> z;
    if ( (x + y + z) != 180 || (x == 0 || y == 0 || z == 0))
        cout << "These numbers don't seem like the three angles of a triangle. ";
    else if ( x == y && y == z && x == z && (x + y + z) == 180)
        cout << "These numbers seem like the three angles of an equilateral triangle.";
    else if ( (x == y || y == z || z == x)  &&  (x + y + z) == 180)
        cout << "These numbers seem like the three angles of an isosceles triangle.";
    else if ( (x == 90 || y == 90 || z == 90) && (x + y + z) == 180)
        cout << "These number seem like the three angles of a right triangle.";
    else if ((x != 90 || y != 90 || z != 90) && (x != y || y != z || z != x) && (x + y + z) == 180)
        cout << "These number seem like the three angles of a triangle.";

    return 0;

}
